<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include
            domain="sharedpref"
            path="." />
        <include
            domain="file"
            path="AI_ARTICLE_SUMMARISER_DATASTORE_FILE.preferences_pb" />
        <include
            domain="database"
            path="ARTICLE_TABLE.db" />
        <include
            domain="database"
            path="ARTICLE_TABLE.db-wal" />
        <include
            domain="database"
            path="ARTICLE_TABLE.db-shm" />
        <exclude
            domain="sharedpref"
            path="device.xml" />
    </cloud-backup>
    <device-transfer>
        <include
            domain="file"
            path="AI_ARTICLE_SUMMARISER_DATASTORE_FILE.preferences_pb" />
        <include
            domain="database"
            path="ARTICLE_TABLE.db" />
        <include
            domain="database"
            path="ARTICLE_TABLE.db-wal" />
        <include
            domain="database"
            path="ARTICLE_TABLE.db-shm" />
        <include
            domain="sharedpref"
            path="." />
        <exclude
            domain="sharedpref"
            path="device.xml" />
    </device-transfer>

</data-extraction-rules>